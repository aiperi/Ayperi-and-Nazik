<p id="notice"><%= notice %></p>

<h1>Listing Vacancies</h1>

<table>
  <thead>
    <tr>
      <th>Email</th>
      <th>Position</th>
      <th>Workplace</th>
      <th>Workingstatus</th>
      <th>Description</th>
      <th>Contactphone</th>
      <th>Deadlinedate</th>
      <th>Salary</th>
      <th>Companyinfo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @vacancies.each do |vacancy| %>
      <tr>
        <td><%= vacancy.email %></td>
        <td><%= vacancy.position %></td>
        <td><%= vacancy.workPlace %></td>
        <td><%= vacancy.workingStatus %></td>
        <td><%= vacancy.description %></td>
        <td><%= vacancy.contactphone %></td>
        <td><%= vacancy.deadlinedate %></td>
        <td><%= vacancy.salary %></td>
        <td><%= vacancy.companyInfo %></td>
        <td><%= link_to 'Show', vacancy %></td>
        <td><%= link_to 'Edit', edit_vacancy_path(vacancy) %></td>
        <td><%= link_to 'Destroy', vacancy, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Vacancy', new_vacancy_path %>
